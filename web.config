<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="rule 1H" stopProcessing="true">
                	<match url="^(.*)$"  />
                	<action type="Rewrite" url="/public/{R:1}"  />
                </rule>
            </rules>
        </rewrite>
        <staticContent>
            <mimeMap fileExtension=".woff" mimeType="font/x-woff" />
            <mimeMap fileExtension=".woff2" mimeType="font/x-woff2" />
          </staticContent>
    </system.webServer>
</configuration>
